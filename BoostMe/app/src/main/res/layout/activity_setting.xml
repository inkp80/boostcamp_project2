<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:id="@+id/setting_tag"
        android:paddingLeft="@dimen/left_margin"
        android:paddingTop="@dimen/top_bot_margin"
        android:paddingBottom="@dimen/top_bot_margin"
        android:textSize="@dimen/menu_font_size"
        android:text="태그 등록하기"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/box_border_bot"/>
    <TextView
        android:id="@+id/setting_location"
        android:paddingLeft="@dimen/left_margin"
        android:paddingTop="@dimen/top_bot_margin"
        android:paddingBottom="@dimen/top_bot_margin"
        android:text="위치 설정하기"
        android:textSize="@dimen/menu_font_size"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/box_border_bot"/>
    <TextView
        android:id="@+id/setting_application_information"
        android:paddingLeft="@dimen/left_margin"
        android:paddingTop="@dimen/top_bot_margin"
        android:paddingBottom="@dimen/top_bot_margin"
        android:text="어플리케이션 정보"
        android:textSize="@dimen/menu_font_size"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/box_border_bot"
        />
</LinearLayout>